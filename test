                Thread.sleep(2000);



                driver.findElement(By.xpath("//*[@id='SearchBtn']")).click();



                Thread.sleep(25000);

                String DepartureTime = driver.findElement(By.xpath("//*[@id=\'flightForm\']/section[2]/div[4]/div[1]/nav/ul/li[1]/div/label/table/tbody/tr[1]/th[3]")).getText();

                System.out.println("Departure Time= "+DepartureTime);

                String ArrivalTime = driver.findElement(By.xpath("//*[@id=\"flightForm\"]/section[2]/div[4]/div[1]/nav/ul/li[1]/div/label/table/tbody/tr[1]/th[4]")).getText();

                System.out.println("Arrival Time= "+ArrivalTime);

        }

        @AfterMethod

         public void exit()

         {

                driver.quit();

         }



}



